    "@mantine/notifications": "4.2.2",
    "@mantine/prism": "4.2.2",
    "@modulz/radix-icons": "^4.0.0",
    "@next/bundle-analyzer": "^12.2.2",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.3",
    "next": "12.1.4",
    "prism-react-renderer": "^1.3.5",
    "react": "18.1.0",
    "react-dom": "18.1.0"
